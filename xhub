local LoadFunction = (function()
    local SuccessValue, ErrorMessage1 = pcall(function()
        local HttpGetResponse = game:HttpGet([[https://raw.githubusercontent.com/Young2x/Flytomoon/refs/heads/main/juice]])
        local LoadedScript = HttpGetResponse and (HttpGetResponse ~= '' and loadstring(HttpGetResponse))

        if LoadedScript then
            return LoadedScript()
        else
            return false
        end
    end)

    if not SuccessValue then
        print('[ Xhub ] VIP Check: Connection Failed')

        return false
    end
    if type(ErrorMessage1) ~= 'table' then
        print('[ Xhub ] VIP Check: Invalid Data')

        return false
    end

    local UserIdString = tostring(LocalPlayer.UserId)
    local PairKey, PairValue, PairIndex = pairs(ErrorMessage1)

    while true do
        local UnusedVariable

        PairIndex, UnusedVariable = PairKey(PairValue, PairIndex)

        if PairIndex == nil then
            break
        end
        if tostring(UnusedVariable) == UserIdString then
            print('[ Xhub ] VIP Check: Verified \u{2713}')

            return true
        end
    end

    print('[ Xhub ] VIP Check: Standard User')

    return false
end)()
