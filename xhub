    return nil
end
local function Function11(Parameter8)
    local PlayerService4 = PlayerService
    local PlayerArray4, PlayerIndex4, PlayerValue4 = ipairs(PlayerService4:GetPlayers())

    while true do
        local PlayerList5

        PlayerValue4, PlayerList5 = PlayerArray4(PlayerIndex4, PlayerValue4)

        if PlayerValue4 == nil then
            break
        end
        if PlayerList5.Name:lower():find(Parameter8:lower()) or PlayerList5.DisplayName:lower():find(Parameter8:lower()) then
            return PlayerList5
        end
    end

    return nil
end
local function Function12(Parameter9)
    GameOption4 = Parameter9

    if Parameter9 then
        task.spawn(function()
            while GameOption4 do
                task.wait(0.3)

                local KnifeObject = LocalPlayer.Backpack:FindFirstChild('Knife') or CharacterModel:FindFirstChild('Knife')

                if KnifeObject and KnifeObject:FindFirstChild('Throw') then
                    local PlayerService5 = PlayerService
                    local PlayerArray5, PlayerIndex5, PlayerValue5 = ipairs(PlayerService5:GetPlayers())

                    while true do
                        local CharacterModel1

                        PlayerValue5, CharacterModel1 = PlayerArray5(PlayerIndex5, PlayerValue5)

                        if PlayerValue5 == nil then
                            break
                        end
                        if CharacterModel1 ~= LocalPlayer and CharacterModel1.Character and (CharacterModel1.Character:FindFirstChild('HumanoidRootPart') and Function2(CharacterModel1)) then
                            local CharacterPart1 = CharacterModel1.Character.HumanoidRootPart

                            if (HumanoidRootPart.Position - CharacterPart1.Position).Magnitude <= KnockbackAmount then
                                local PositionOffset = CharacterPart1.Position + Vector3.new(0, 1, 0)
                                local CFrameList = {
                                    CFrame.new(HumanoidRootPart.Position, PositionOffset),
                                    PositionOffset,
                                }

                                KnifeObject.Throw:FireServer(unpack(CFrameList))
                            end
                        end
                    end
                end
            end
        end)
    end
end
local function Function1()
    local KnifeObject = LocalPlayer.Character:FindFirstChild('Knife') or LocalPlayer.Backpack:FindFirstChild('Knife')

    if KnifeObject then
        if LocalPlayer.Backpack:FindFirstChild('Knife') then
            LocalPlayer.Character.Humanoid:EquipTool(KnifeObject)
            task.wait(0.3)
        end

        local KnifeHandle = KnifeObject:FindFirstChild('Handle')

        if KnifeHandle then
            return KnifeObject, KnifeHandle
        else
            return nil, nil
        end
    else
        return nil, nil
    end
end
local function Function2(Parameter1)
    local PlayerService = PlayerService
    local Player1, Player2, Player3 = pairs(PlayerService:GetPlayers())

    while true do
        local Character1

        Player3, Character1 = Player1(Player2, Player3)

        if Player3 == nil then
            break
        end

        local CharacterModel = Character1.Character

        if CharacterModel and (CharacterModel:FindFirstChild(Parameter1) or CharacterModel:FindFirstChild(Parameter1, true)) then
            local Humanoid1 = CharacterModel:FindFirstChildWhichIsA('Humanoid')

            if Humanoid1 then
                CurrentCamera.CameraSubject = Humanoid1

                return true
            end
        end
    end

    return false
end
local function Function3()
    local LocalPlayer = game.Players.LocalPlayer
    local LocalCharacter = not LocalPlayer.Backpack:FindFirstChild('Knife') and LocalPlayer.Character

    if LocalCharacter then
        LocalCharacter = LocalPlayer.Character:FindFirstChild('Knife')
    end

    return LocalCharacter
end
local function Function4()
    while true do
        if not GameOption5 then
            return
        end

        local LocalCharacterModel = game.Players.LocalPlayer.Character

        if LocalCharacterModel then
            local HumanoidRootPart1 = LocalCharacterModel:FindFirstChild('HumanoidRootPart')

            if HumanoidRootPart1 and not Function3() then
                local Descendant1, Descendant2, Descendant3 = pairs(game.Workspace:GetDescendants())

                while true do
                    local Object1

                    Descendant3, Object1 = Descendant1(Descendant2, Descendant3)

                    if Descendant3 == nil then
                        break
                    end
                    if Object1:IsA('Part') and (Object1.Name == 'GunDrop' and Object1.Parent) and Object1.Parent:IsA('Model') then
                        firetouchinterest(HumanoidRootPart1, Object1, 0)
                        firetouchinterest(HumanoidRootPart1, Object1, 1)

                        break
                    end
                end
            end
        end

        wait(0.3)
    end
end
local function Function5()
    while GameOption6 do
        local Player2 = game.Players.LocalPlayer

        if not (Player2 and Player2.Character) then
            return
        end
        if Function9() == Player2 then
            local CharacterModel2 = Player2.Character
            local Humanoid2 = CharacterModel2:FindFirstChild('Humanoid')
            local KnifeObject2 = CharacterModel2:FindFirstChild('Knife') or Player2.Backpack:FindFirstChild('Knife')

            if KnifeObject2 and Humanoid2 then
                Humanoid2:EquipTool(KnifeObject2)

                local Player4, Player5, Player6 = ipairs(game.Players:GetPlayers())

                while true do
                    local Object2

                    Player6, Object2 = Player4(Player5, Player6)

                    if Player6 == nil then
                        break
                    end
                    if Object2 ~= Player2 and Object2.Character and Object2.Character:FindFirstChild('HumanoidRootPart') then
                        Object2.Character.HumanoidRootPart.CFrame = CharacterModel2.HumanoidRootPart.CFrame + CharacterModel2.HumanoidRootPart.CFrame.LookVector * 2
                    end
                end

                KnifeObject2.Stab:FireServer(unpack({
                    'Slash',
                }))
            end
        end

        task.wait(0.5)
    end
end
local function Function6()
    local CharacterModel3 = LocalPlayer.Character

    if CharacterModel3 then
        CharacterModel3 = LocalPlayer.Character:FindFirstChildOfClass('Humanoid')
    end
    if CharacterModel3 then
        CharacterModel3.WalkSpeed = JumpPower and SixteenValue or 16
        CharacterModel3.JumpPower = JumpPower1 and FiftyValue or 50
    end
end

