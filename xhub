local GunDropConfig = {
    Name = 'GunDrop',
    Callback = function()
        local WorkspaceService3 = Workspace
        local WorkspaceDescendant10, WorkspaceDescendant11, WorkspaceDescendant12 = ipairs(WorkspaceService3:GetDescendants())
        local FalseValue = false

        while true do
            local UnusedVariable8

            WorkspaceDescendant12, UnusedVariable8 = WorkspaceDescendant10(WorkspaceDescendant11, WorkspaceDescendant12)

            if WorkspaceDescendant12 == nil then
                break
            end
            if UnusedVariable8:IsA('Part') and UnusedVariable8.Name == 'GunDrop' then
                local CharacterCFrame = HumanoidRootPart.CFrame
                local AdjustedCFrame = UnusedVariable8.CFrame + Vector3.new(0, -2, 0)
                local Value10 = 10

                FalseValue = true

                for LoopCounter = 1, Value10 do
                    HumanoidRootPart.CFrame = CharacterCFrame:Lerp(AdjustedCFrame, LoopCounter / Value10)

                    task.wait(0.01)
                end

                HumanoidRootPart.CFrame = CharacterCFrame

                break
            end
        end

        if not FalseValue then
            MakeNotifi({
                Title = 'Gun Not Found',
                Text = '',
                Time = 3,
            })
        end
    end,
}

AddButton(Main, GunDropConfig)

