AddButton(Main, {
    Name = 'Notify Murderer & Sheriff',
    Callback = function()
        local PlayerList = PlayerService:GetPlayers()
        local Player1, Player2, Player3 = ipairs(PlayerList)
        local UnknownValue4 = nil
        local UnknownValue5 = nil

        while true do
            local UnknownValue6

            Player3, UnknownValue6 = Player1(Player2, Player3)

            if Player3 == nil then
                break
            end
            if UnknownValue6.Backpack:FindFirstChild('Knife') then
                UnknownValue4 = UnknownValue6
            elseif UnknownValue6.Character then
                if UnknownValue6.Character:FindFirstChild('Knife') then
                    UnknownValue4 = UnknownValue6
                end
            end
            if UnknownValue6.Backpack:FindFirstChild('Gun') then
                UnknownValue5 = UnknownValue6
            elseif UnknownValue6.Character then
                if UnknownValue6.Character:FindFirstChild('Gun') then
                    UnknownValue5 = UnknownValue6
                end
            end
        end

        if UnknownValue4 or UnknownValue5 then
            if UnknownValue4 then
                StarterGui:SetCore('SendNotification', {
                    Title = 'The Murderer is: ',
                    Text = UnknownValue4.Name,
                    Icon = [[http://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&username=]] .. UnknownValue4.Name,
                    Duration = 6,
                })
            end
            if UnknownValue5 then
                StarterGui:SetCore('SendNotification', {
                    Title = 'The Sheriff is:',
                    Text = UnknownValue5.Name,
                    Icon = [[http://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&username=]] .. UnknownValue5.Name,
                    Duration = 6,
                })
            end
        else
            MakeNotifi({
                Title = 'No Murderer or Sheriff found',
                Time = 3,
            })
        end
    end,
})

local SendMurdererSetting = {
    Name = 'Send Murderer & Sheriff in chat',
    Callback = function()
        task.spawn(function()
            local FunctionResult2 = Function9()
            local FunctionResult3 = Function10()
            local NameOrUnknown = FunctionResult2 and FunctionResult2.Name or 'Unknown'
            local NameOrUnknown2 = FunctionResult3 and FunctionResult3.Name or 'Unknown'

            if TextChatService and (TextChatService.TextChannels and TextChatService.TextChannels.RBXGeneral) then
                local ChatMessage = pcall(function()
                    TextChatService.TextChannels.RBXGeneral:SendAsync('Sheriff: ' .. NameOrUnknown2)
                end)

                task.wait(0.5)

                if not (ChatMessage and pcall(function()
                    TextChatService.TextChannels.RBXGeneral:SendAsync('Murderer: ' .. NameOrUnknown)
                end)) then
                    MakeNotifi({
                        Title = 'Wait pls!',
                        Text = '',
                        Time = 3,
                    })
                end
            else
                MakeNotifi({
                    Title = 'where the chat?',
                    Text = '',
                    Time = 3,
                })
            end
        end)
    end,
}

