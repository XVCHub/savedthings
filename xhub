local AboveMapConfig = {
    Name = 'Above Map',
    Callback = function()
        local WorkspaceService2 = Workspace
        local WorkspaceDescendant7, WorkspaceDescendant8, WorkspaceDescendant9 = pairs(WorkspaceService2:GetDescendants())
        local EmptyTable = {}

        while true do
            local UnusedVariable5

            WorkspaceDescendant9, UnusedVariable5 = WorkspaceDescendant7(WorkspaceDescendant8, WorkspaceDescendant9)

            if WorkspaceDescendant9 == nil then
                break
            end
            if UnusedVariable5.Name == 'Spawn' or UnusedVariable5.Name == 'PlayerSpawn' then
                table.insert(EmptyTable, UnusedVariable5)
            end
        end

        if #EmptyTable <= 0 then
            warn('oh.. ')
        else
            HumanoidRootPart.CFrame = EmptyTable[math.random(1, #EmptyTable)].CFrame * CFrame.new(0, 150, 0)
        end
    end,
}

