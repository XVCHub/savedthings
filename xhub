local GetDataFunction = ReplicatedStorage:FindFirstChild('GetPlayerData', true)

local function LocalFunction()
    task.wait(0.5)

    local PlayerData = GetDataFunction:InvokeServer()
    local PlayerRole = GameOptions.SRz and PlayerData[LocalPlayer.Name]

    if PlayerRole then
        local RoleType = PlayerRole.Role
        local ColorTable = ({
            Murderer = Color3.fromRGB(255, 0, 0),
            Sheriff = Color3.fromRGB(0, 0, 255),
            Innocent = Color3.fromRGB(0, 255, 0),
        })[RoleType]

        if ColorTable then
            if LocalPlayer:FindFirstChild('PlayerGui'):FindFirstChild('RoleGui') then
                LocalPlayer.PlayerGui.RoleGui:Destroy()
            end

            local ScreenGui = Instance.new('ScreenGui', LocalPlayer.PlayerGui)

            ScreenGui.Name = 'RoleGui'
            ScreenGui.ResetOnSpawn = false

            local TextLabel = Instance.new('TextLabel', ScreenGui)

            TextLabel.Name = 'RoleText'
            TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
            TextLabel.Position = UDim2.fromScale(0.5, 0.22)
            TextLabel.Size = UDim2.new(0, 250, 0, 50)
            TextLabel.BackgroundTransparency = 1
            TextLabel.Text = RoleType
            TextLabel.TextSize = 39
            TextLabel.Font = Enum.Font.SourceSansBold
            TextLabel.TextColor3 = ColorTable

            task.delay(5, function()
                if ScreenGui then
                    ScreenGui:Destroy()
                end
            end)
        end
    end
    if GameOptions.SRP then
        local PlayerList1, PlayerIndex1, PlayerKey1 = pairs(PlayerData)

        while true do
            local UnusedVariable1

            PlayerKey1, UnusedVariable1 = PlayerList1(PlayerIndex1, PlayerKey1)

            if PlayerKey1 == nil then
                break
            end
            if UnusedVariable1.Role == 'Murderer' or UnusedVariable1.Role == 'Sheriff' then
                local PlayerObject = PlayerService:FindFirstChild(PlayerKey1)

                if PlayerObject then
                    local PlayerAvatar = 'https://www.roblox.com/headshot-thumbnail/image?userId=' .. PlayerObject.UserId .. '&width=150&height=150&format=png'

                    StarterGui:SetCore('SendNotification', {
                        Title = UnusedVariable1.Role,
                        Text = PlayerKey1,
                        Icon = PlayerAvatar,
                        Duration = 6,
                    })
                end
            end
        end
    end
end

ReplicatedStorage:WaitForChild('Remotes'):WaitForChild('Gameplay'):WaitForChild('RoleSelect').OnClientEvent:Connect(LocalFunction)
