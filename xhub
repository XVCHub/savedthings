_G.ButtonSize = 70
_G.ButtonTransparency = 1

local UnusedFunction1 = nil
local UnusedFunction2 = nil
local UnknownValue = nil

local function FunctionHandler()
    if UnusedFunction2 and UnknownValue then
        UnusedFunction2.Size = UDim2.new(0, _G.ButtonSize, 0, _G.ButtonSize)

        if _G.ButtonTransparency ~= 1 then
            if _G.ButtonTransparency ~= 2 then
                UnusedFunction2.BackgroundTransparency = _G.ButtonTransparency - 1
                UnusedFunction2.ImageTransparency = _G.ButtonTransparency - 1
                UnknownValue.Transparency = 0
            else
                UnusedFunction2.BackgroundTransparency = 1
                UnusedFunction2.ImageTransparency = 1
                UnknownValue.Transparency = 0
            end
        else
            UnusedFunction2.BackgroundTransparency = 0
            UnusedFunction2.ImageTransparency = 0
            UnknownValue.Transparency = 0
        end
    end
end

local ShootOffsetValue = 2.8

task.spawn(function()
    repeat
        task.wait()
    until game.Players.LocalPlayer:FindFirstChild('PlayerGui')

    local PlayerGuiInstance = game.Players.LocalPlayer:WaitForChild('PlayerGui')

    UnusedFunction1 = Instance.new('ScreenGui')
    UnusedFunction2 = Instance.new('ImageButton')

    local UICornerInstance = Instance.new('UICorner')

    UnknownValue = Instance.new('UIStroke')
    UnusedFunction1.Name = 'ShootGui'
    UnusedFunction1.ResetOnSpawn = false
    UnusedFunction1.Parent = PlayerGuiInstance
    UnusedFunction2.Name = 'ShootButton'
    UnusedFunction2.Parent = UnusedFunction1
    UnusedFunction2.BackgroundColor3 = Color3.new(0, 0, 0)
    UnusedFunction2.Position = UDim2.new(1, -10, 0.224, 0)
    UnusedFunction2.Size = UDim2.new(0, getgenv().ButtonSize, 0, getgenv().ButtonSize)
    UnusedFunction2.Image = 'rbxthumb://type=Asset&id=83449867941817&w=150&h=150'
    UnusedFunction2.AnchorPoint = Vector2.new(1, 0)
    UnusedFunction2.Draggable = true
    UnusedFunction2.Visible = false
    UICornerInstance.CornerRadius = UDim.new(0, 23)
    UICornerInstance.Parent = UnusedFunction2
    UnknownValue.Color = Color3.fromRGB(57, 57, 57)
    UnknownValue.Thickness = 2
    UnknownValue.Parent = UnusedFunction2

    FunctionHandler()

    local LocalPlayerInstance = game.Players.LocalPlayer

    local function Function1()
        local function Function2(CharacterParameter, Parameter2)
            if not (CharacterParameter and CharacterParameter.Character) then
                return Vector3.zero
            end

            local CharacterModel = CharacterParameter.Character
            local UpperTorsoPart = CharacterModel:FindFirstChild('UpperTorso')
            local HumanoidInstance = CharacterModel:FindFirstChild('Humanoid')

            if not (UpperTorsoPart and HumanoidInstance) then
                return Vector3.zero
            end

            local AssemblyLinearVelocity = UpperTorsoPart.AssemblyLinearVelocity
            local MoveDirectionVector = HumanoidInstance.MoveDirection

            return UpperTorsoPart.Position + AssemblyLinearVelocity * Vector3.new(0, 0.5, 0) * (Parameter2 / 15) + MoveDirectionVector * Parameter2
        end

        if Function10() == LocalPlayerInstance then
            local FunctionResult = Function9()

            if FunctionResult then
                if not LocalPlayerInstance.Character:FindFirstChild('Gun') then
                    local HumanoidComponent = LocalPlayerInstance.Character:FindFirstChild('Humanoid')

                    if not LocalPlayerInstance.Backpack:FindFirstChild('Gun') then
                        MakeNotifi({
                            Title = 'You dont have a gun',
                            Text = '',
                            Time = 3,
                        })

                        return
                    end

                    HumanoidComponent:EquipTool(LocalPlayerInstance.Backpack:FindFirstChild('Gun'))
                end

                local TableData = {
                    1,
                    Function2(FunctionResult, ShootOffsetValue),
                    'AH2',
                }

                LocalPlayerInstance.Character.Gun.KnifeLocal.CreateBeam.RemoteFunction:InvokeServer(unpack(TableData))
            end
        else
            MakeNotifi({
                Title = 'You dont have a gun',
                Text = '',
                Time = 3,
            })

            return
        end
    end

    UnusedFunction2.MouseButton1Click:Connect(Function1)
end)

local SettingData = {
    Name = 'Shoot Murderer',
    Default = false,
    Callback = function(SettingParameter)
        if UnusedFunction2 then
            UnusedFunction2.Visible = SettingParameter
        end
    end,
}

AddToggle(Main, SettingData)

