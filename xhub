local function Function1(Parameter1)
    if Parameter1.Character then
        if Parameter1.Character:FindFirstChild('Knife') then
            return Color3.fromRGB(225, 0, 0)
        end
        if Parameter1.Character:FindFirstChild('Gun') then
            return Color3.fromRGB(0, 0, 225)
        end
    end
    if Parameter1.Backpack then
        if Parameter1.Backpack:FindFirstChild('Knife') then
            return Color3.fromRGB(225, 0, 0)
        end
        if Parameter1.Backpack:FindFirstChild('Gun') then
            return Color3.fromRGB(0, 0, 225)
        end
    end
    if EmptyTable and type(EmptyTable) == 'table' then
        local TableKey, TableValue, TableIndex = pairs(EmptyTable)

        while true do
            local UnusedVariable1

            TableIndex, UnusedVariable1 = TableKey(TableValue, TableIndex)

            if TableIndex == nil then
                break
            end
            if Parameter1.Name == TableIndex and (UnusedVariable1 and UnusedVariable1.Role) then
                if UnusedVariable1.Role ~= 'Murderer' then
                    if UnusedVariable1.Role ~= 'Sheriff' then
                        if UnusedVariable1.Role ~= 'Hero' then
                            return Color3.fromRGB(0, 255, 0)
                        else
                            return Color3.fromRGB(255, 250, 0)
                        end
                    else
                        return Color3.fromRGB(0, 0, 225)
                    end
                else
                    return Color3.fromRGB(225, 0, 0)
                end
            end
        end
    end

    return Color3.fromRGB(0, 255, 0)
end
local function Function2(Parameter2)
    if not EmptyTable or (type(EmptyTable) ~= 'table' or not next(EmptyTable)) then
        return true
    end

    local TableKey1, TableValue1, TableIndex1 = pairs(EmptyTable)

    while true do
        local UnusedVariable2

        TableIndex1, UnusedVariable2 = TableKey1(TableValue1, TableIndex1)

        if TableIndex1 == nil then
            break
        end
        if Parameter2.Name == TableIndex1 and UnusedVariable2 then
            local UnusedVariable3

            if UnusedVariable2.Killed then
                UnusedVariable3 = false
            else
                UnusedVariable3 = not UnusedVariable2.Dead
            end

            return UnusedVariable3
        end
    end

    return true
end

RunService.RenderStepped:Connect(function()
    local TickCount = tick()

    if OneValue <= TickCount - ZeroValue then
        ZeroValue = TickCount

        local GetPlayerDataScript, GetPlayerDataResult = pcall(function()
            local GetPlayerDataModule = ReplicatedStorage:FindFirstChild('GetPlayerData', true)

            if GetPlayerDataModule and typeof(GetPlayerDataModule.InvokeServer) == 'function' then
                return GetPlayerDataModule:InvokeServer()
            else
                return nil
            end
        end)

        if GetPlayerDataScript and (GetPlayerDataResult and type(GetPlayerDataResult) == 'table') then
            EmptyTable = GetPlayerDataResult
        elseif not EmptyTable then
            EmptyTable = {}
        end
    end
end)

local function Function3(Parameter3)
    if not (Parameter3 and Parameter3.Character) then
        return 0
    end

    local CharacterModel3 = LocalPlayer.Character

    if not CharacterModel3 then
        return 0
    end
    if CharacterModel3 then
        CharacterModel3 = CharacterModel3:FindFirstChild('HumanoidRootPart')
    end

    local CharacterModel4 = Parameter3.Character

    if CharacterModel4 then
        CharacterModel4 = CharacterModel4:FindFirstChild('HumanoidRootPart')
    end

    return not (CharacterModel3 and CharacterModel4) and 0 or (CharacterModel3.Position - CharacterModel4.Position).Magnitude
end
local function Function4()
    if LocalPlayer and LocalPlayer.Character then
        local PlayerService1 = PlayerService
        local ChildKey, ChildValue, ChildIndex = pairs(PlayerService1:GetChildren())

        while true do
            local Orb

            ChildIndex, Orb = ChildKey(ChildValue, ChildIndex)

            if ChildIndex == nil then
                break
            end
            if Orb ~= LocalPlayer and (Orb and Orb.Character) then
                local ColorCheckResult, ShootOffsetValue = pcall(function()
                    local ColorValue = Function1(Orb)
                    local RedColor = ColorValue == Color3.fromRGB(225, 0, 0)
                    local BlueColor = ColorValue == Color3.fromRGB(0, 0, 225)
                    local GreenColor = ColorValue == Color3.fromRGB(0, 255, 0)
                    local AllSetting = Settings.names.all

                    if not AllSetting then
                        if Settings.names.mrd and RedColor then
                            AllSetting = RedColor
                        elseif Settings.names.shf and BlueColor then
                            AllSetting = BlueColor
                        else
                            AllSetting = Settings.names.inn

                            if AllSetting then
                                AllSetting = GreenColor
                            end
                        end
                    end

                    local AllColor = Settings.color.all

                    if not AllColor then
                        if Settings.color.mrd and RedColor then
                            AllColor = RedColor
                        elseif Settings.color.shf and BlueColor then
                            AllColor = BlueColor
                        else
                            AllColor = Settings.color.inn

                            if AllColor then
                                AllColor = GreenColor
                            end
                        end
                    end

                    local AllOutline = Settings.oln.all

                    if AllOutline then
                        GreenColor = AllOutline
                    elseif Settings.oln.mrd and RedColor then
                        GreenColor = RedColor
                    elseif Settings.oln.shf and BlueColor then
                        GreenColor = BlueColor
                    else
                        local InnOutline = Settings.oln.inn

                        if not InnOutline then
                            GreenColor = InnOutline
                        end
                    end

                    local Orb1 = Orb.Character
                    local HeadPart = Orb1:FindFirstChild('Head')

                    if AllSetting and HeadPart then
                        if not Orb1:FindFirstChild('NameTag') then
                            local NameTagGui = Instance.new('BillboardGui', Orb1)

                            NameTagGui.Name = 'NameTag'
                            NameTagGui.Size = UDim2.new(0, 200, 0, 50)
                            NameTagGui.Adornee = HeadPart
                            NameTagGui.AlwaysOnTop = true

                            local NameTagLabel = Instance.new('TextLabel', NameTagGui)

                            NameTagLabel.Size = UDim2.new(1, 0, 1, 0)
                            NameTagLabel.BackgroundTransparency = 1
                            NameTagLabel.TextStrokeTransparency = 0.5
                            NameTagLabel.Font = Enum.Font.SourceSans
                            NameTagLabel.TextSize = 15
                            NameTagLabel.TextColor3 = ColorValue
                        end

                        local NameTag = Orb1:FindFirstChild('NameTag')

                        if NameTag and NameTag:FindFirstChild('TextLabel') then
                            local HealthValue = math.floor(Function3(Orb))

                            NameTag.TextLabel.Text = ' ( ' .. HealthValue .. ' ) ' .. Orb.Name
                            NameTag.TextLabel.TextColor3 = ColorValue
                        end
                    else
                        local NameTag1 = Orb1:FindFirstChild('NameTag')

                        if NameTag1 then
                            NameTag1:Destroy()
                        end
                    end
                    if AllColor then
                        if not Orb1:FindFirstChild('Highlight') then
                            local Highlight = Instance.new('Highlight', Orb1)

                            Highlight.FillTransparency = 0.5
                            Highlight.OutlineTransparency = 1
                        end

                        local Highlight1 = Orb1:FindFirstChild('Highlight')

                        if Highlight1 then
                            Highlight1.FillColor = ColorValue
                        end
                    else
                        local Highlight2 = Orb1:FindFirstChild('Highlight')

                        if Highlight2 then
                            Highlight2:Destroy()
                        end
                    end
                    if GreenColor then
                        if not Orb1:FindFirstChild('OH') then
                            local Highlight3 = Instance.new('Highlight', Orb1)

                            Highlight3.Name = 'OH'
                            Highlight3.FillTransparency = 1
                            Highlight3.OutlineTransparency = 0
                        end

                        local OhPart = Orb1:FindFirstChild('OH')

                        if OhPart then
                            OhPart.OutlineColor = ColorValue
                        end
                    else
                        local ObjectHandle = Orb1:FindFirstChild('OH')

                        if ObjectHandle then
                            ObjectHandle:Destroy()
                        end
                    end
                end)

                if not ColorCheckResult then
                    warn('ESP Error')
                end
            end
        end
    end
end
local function Function1()
    local PlayerList = PlayerService
    local PlayerIterator, PlayerData, PlayerInfo = pairs(PlayerList:GetPlayers())

    while true do
        local CharacterModel

        PlayerInfo, CharacterModel = PlayerIterator(PlayerData, PlayerInfo)

        if PlayerInfo == nil then
            break
        end
        if CharacterModel == LocalPlayer or not CharacterModel.Character or not CharacterModel.Character:FindFirstChild('HumanoidRootPart') then
            if TrackingData.trc[CharacterModel.Name] then
                TrackingData.trc[CharacterModel.Name]:Remove()

                TrackingData.trc[CharacterModel.Name] = nil
            end
            if TrackingData.box[CharacterModel.Name] then
                TrackingData.box[CharacterModel.Name]:Remove()

                TrackingData.box[CharacterModel.Name] = nil
            end
        else
            local CharacterPart = CharacterModel.Character.HumanoidRootPart
            local ViewportPosition, ScreenPosition = Workspace.CurrentCamera:WorldToViewportPoint(CharacterPart.Position)
            local ColorValue = Function1(CharacterModel)
            local RedColor = ColorValue == Color3.fromRGB(225, 0, 0)
            local BlueColor = ColorValue == Color3.fromRGB(0, 0, 225)
            local GreenColor = ColorValue == Color3.fromRGB(0, 255, 0)

            if Settings.trc.all or Settings.trc.mrd and RedColor or (Settings.trc.shf and BlueColor or Settings.trc.inn and GreenColor) then
                if not TrackingData.trc[CharacterModel.Name] then
                    TrackingData.trc[CharacterModel.Name] = Drawing.new('Line')
                    TrackingData.trc[CharacterModel.Name].Thickness = 2
                    TrackingData.trc[CharacterModel.Name].Transparency = 1
                end

                local TraceData = TrackingData.trc[CharacterModel.Name]

                if TraceData then
                    TraceData.Color = ColorValue
                    TraceData.From = Vector2.new(Workspace.CurrentCamera.ViewportSize.X / 2, Workspace.CurrentCamera.ViewportSize.Y)
                    TraceData.To = Vector2.new(ViewportPosition.X, ViewportPosition.Y)
                    TraceData.Visible = ScreenPosition
                end
            elseif TrackingData.trc[CharacterModel.Name] then
                TrackingData.trc[CharacterModel.Name]:Remove()

                TrackingData.trc[CharacterModel.Name] = nil
            end
            if Settings.box.all or Settings.box.mrd and RedColor or (Settings.box.shf and BlueColor or Settings.box.inn and GreenColor) then
                if not TrackingData.box[CharacterModel.Name] then
                    TrackingData.box[CharacterModel.Name] = Drawing.new('Square')
                    TrackingData.box[CharacterModel.Name].Thickness = 2
                    TrackingData.box[CharacterModel.Name].Transparency = 1
                end

                local BoxData = TrackingData.box[CharacterModel.Name]

                if BoxData then
                    local WindowSize = Vector2.new(60, 60)
                    local WindowPosition = Vector2.new(ViewportPosition.X - WindowSize.X / 2, ViewportPosition.Y - WindowSize.Y / 2)

                    BoxData.Color = ColorValue
                    BoxData.Position = WindowPosition
                    BoxData.Size = WindowSize
                    BoxData.Visible = ScreenPosition
                end
            elseif TrackingData.box[CharacterModel.Name] then
                TrackingData.box[CharacterModel.Name]:Remove()

                TrackingData.box[CharacterModel.Name] = nil
            end
        end
    end
end
local function Function2(Parameter1, Parameter2, Parameter3)
    if Parameter1 == 'all' then
        Settings[Parameter2].all = Parameter3
    elseif Parameter1 == 'mrd' then
        Settings[Parameter2].mrd = Parameter3
    elseif Parameter1 == 'shf' then
        Settings[Parameter2].shf = Parameter3
    elseif Parameter1 == 'inn' then
        Settings[Parameter2].inn = Parameter3
    end
end

PlayerService.PlayerRemoving:Connect(function(Parameter4)
    if TrackingData.trc[Parameter4.Name] then
        TrackingData.trc[Parameter4.Name]:Remove()

        TrackingData.trc[Parameter4.Name] = nil
    end
    if TrackingData.box[Parameter4.Name] then
        TrackingData.box[Parameter4.Name]:Remove()

        TrackingData.box[Parameter4.Name] = nil
    end
end)
