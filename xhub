local UnknownValue12 = nil

function NewButton(Parameter8)
    local ColorData = {49, 49}
    local ColorBlack = Color3.fromRGB(0, 0, 0)
    local ColorWhite = Color3.fromRGB(255, 255, 255)
    local FontSourceSans = Enum.Font.SourceSans
    local UDimValue = UDim.new(0, 21)

    UnknownValue12 = Instance.new('TextButton')
    UnknownValue12.Size = UDim2.new(0, ColorData[1], 0, ColorData[2])
    UnknownValue12.BackgroundColor3 = ColorBlack
    UnknownValue12.BackgroundTransparency = 0.8
    UnknownValue12.Text = Parameter8 or 'Button'
    UnknownValue12.TextColor3 = ColorWhite
    UnknownValue12.Font = FontSourceSans
    UnknownValue12.TextScaled = true
    UnknownValue12.TextWrapped = true
    UnknownValue12.Active = true
    UnknownValue12.Draggable = true
    UnknownValue12.Parent = ScreenGuiInstance
    Instance.new('UICorner', UnknownValue12).CornerRadius = UDimValue
end
function Pos(Parameter9, Parameter10, Parameter11, Parameter12)
    if UnknownValue12 then
        UnknownValue12.Position = UDim2.new(Parameter9, Parameter10, Parameter11, Parameter12)
    end
end
function Function(Parameter13)
    if UnknownValue12 then
        UnknownValue12.MouseButton1Click:Connect(Parameter13)
    end
end

