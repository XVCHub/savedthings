local MapConfig = {
    Name = 'Map',
    Callback = function()
        local WorkspaceService = Workspace
        local WorkspaceDescendant4, WorkspaceDescendant5, WorkspaceDescendant6 = pairs(WorkspaceService:GetDescendants())

        while true do
            local UnusedVariable4

            WorkspaceDescendant6, UnusedVariable4 = WorkspaceDescendant4(WorkspaceDescendant5, WorkspaceDescendant6)

            if WorkspaceDescendant6 == nil then
                break
            end
            if UnusedVariable4.Name ~= 'Spawn' then
                if UnusedVariable4.Name == 'PlayerSpawn' then
                    HumanoidRootPart.CFrame = CFrame.new(UnusedVariable4.Position) * CFrame.new(0, 2.5, 0)
                end
            else
                HumanoidRootPart.CFrame = CFrame.new(UnusedVariable4.Position) * CFrame.new(0, 2.5, 0)
            end
        end
    end,
}

