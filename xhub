AddToggle(Main, {
    Name = 'Auto Notification Dropped Gun',
    Default = false,
    Callback = function(Parameter3)
        GameOptions.GN4 = Parameter3

        if Parameter3 then
            NilValue1 = Workspace.DescendantAdded:Connect(function(Parameter4)
                if Parameter4.Name == 'GunDrop' and GameOptions.GN4 then
                    MakeNotifi({
                        Title = 'THE GUN DROPPED',
                        Text = '',
                        Time = 3,
                    })
                end
            end)
            NilValue2 = Workspace.DescendantRemoving:Connect(function(Parameter5)
                if Parameter5.Name == 'GunDrop' and GameOptions.GN4 then
                    MakeNotifi({
                        Title = 'The Gun is taken.',
                        Text = '',
                        Time = 3,
                    })
                end
            end)
        else
            if NilValue1 then
                NilValue1:Disconnect()
            end
            if NilValue2 then
                NilValue2:Disconnect()
            end
        end
    end,
})

