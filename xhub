SetSection(TestSection5, 'CUSTOM CURSOR')

local SettingsTable = {
    Settings = {
        Enabled = false,
        Cursor = 'rbxasset://textures/MouseLockedCursor.png',
        Glow = false,
        Pulse = false,
        Spin = false,
        Rotation = 0,
        Size = 30,
    },
    Cursors = {},
    Names = {},
    GlowConn = nil,
    PulseConn = nil,
    SpinConn = nil,
}
local JsonUrl = [[https://raw.githubusercontent.com/Young2x/Flytomoon/refs/heads/main/CRRA.json]]

pcall(function()
    SettingsTable.Cursors = HttpService:JSONDecode((game:HttpGet(JsonUrl)))

    local CursorList, CursorIndex, CursorKey = pairs(SettingsTable.Cursors)

    while true do
        CursorKey = CursorList(CursorIndex, CursorKey)

        if CursorKey == nil then
            break
        end

        table.insert(SettingsTable.Names, CursorKey)
    end
end)

local function LocalFunction1()
    local GameTopbar = (LocalPlayer:FindFirstChild('PlayerGui') or LocalPlayer:WaitForChild('PlayerGui')):FindFirstChild('GameTopbar', true)

    return GameTopbar and GameTopbar:FindFirstChild('Crosshair') or nil
end
local function LocalFunction2()
    local FunctionResult = LocalFunction1()

    if FunctionResult then
        FunctionResult.Image = SettingsTable.Settings.Enabled and SettingsTable.Settings.Cursor or (SettingsTable.Cursors['Cursor Normal'] or 'rbxasset://textures/MouseLockedCursor.png')
        FunctionResult.ImageColor3 = Color3.new(1, 1, 1)
        FunctionResult.ImageTransparency = 0
        FunctionResult.Rotation = 0
        FunctionResult.Size = UDim2.new(0, SettingsTable.Settings.Size, 0, SettingsTable.Settings.Size)
    end
end

local CustomCursorOption = {
    Name = 'Custom Cursor',
    Default = false,
    Callback = function(CursorParameter)
        SettingsTable.Settings.Enabled = CursorParameter

        LocalFunction2()
    end,
}

AddToggle(Main, CustomCursorOption)
AddDropdown(Main, {
    Name = 'Select You Favorite Cursor',
    Options = SettingsTable.Names,
    Default = 'Normal',
    Callback = function(Parameter10)
        SettingsTable.Settings.Cursor = SettingsTable.Cursors[Parameter10] or SettingsTable.Cursors['Cursor Normal']

        LocalFunction2()
    end,
})

local GlowOption = {
    Name = 'Glow',
    Default = false,
    Callback = function(GlowParameter)
        SettingsTable.Settings.Glow = GlowParameter

        if SettingsTable.GlowConn then
            SettingsTable.GlowConn:Disconnect()
        end

        local FunctionResult1 = LocalFunction1()

        if GlowParameter and FunctionResult1 then
            SettingsTable.GlowConn = RunService.RenderStepped:Connect(function()
                local FunctionResult2 = LocalFunction1()

                if FunctionResult2 then
                    FunctionResult2.ImageColor3 = Color3.fromHSV(tick() % 5 / 5, 0.7, 1)
                    FunctionResult2.ImageTransparency = 0.1
                end
            end)
        elseif FunctionResult1 then
            FunctionResult1.ImageColor3 = Color3.new(1, 1, 1)
            FunctionResult1.ImageTransparency = 0
        end
    end,
}

AddToggle(Main, GlowOption)

local PulseOption = {
    Name = 'Pulse',
    Default = false,
    Callback = function(PulseParameter)
        SettingsTable.Settings.Pulse = PulseParameter

        if SettingsTable.PulseConn then
            SettingsTable.PulseConn:Disconnect()
        end

        local FunctionResult3 = LocalFunction1()

        if PulseParameter and FunctionResult3 then
            SettingsTable.PulseConn = RunService.RenderStepped:Connect(function()
                local FunctionResult4 = LocalFunction1()

                if FunctionResult4 then
                    local PulseSize = SettingsTable.Settings.Size + math.sin(tick() * 3) * 4

                    FunctionResult4.Size = UDim2.new(0, PulseSize, 0, PulseSize)
                end
            end)
        elseif FunctionResult3 then
            FunctionResult3.Size = UDim2.new(0, SettingsTable.Settings.Size, 0, SettingsTable.Settings.Size)
        end
    end,
}

AddToggle(Main, PulseOption)

local SpinOption = {
    Name = 'Spin',
    Default = false,
    Callback = function(SpinParameter)
        SettingsTable.Settings.Spin = SpinParameter

        if SettingsTable.SpinConn then
            SettingsTable.SpinConn:Disconnect()
        end

        local FunctionResult5 = LocalFunction1()

        if SpinParameter and FunctionResult5 then
            SettingsTable.SpinConn = RunService.RenderStepped:Connect(function()
                local FunctionResult6 = LocalFunction1()

                if FunctionResult6 then
                    SettingsTable.Settings.Rotation = (SettingsTable.Settings.Rotation + 5) % 360
                    FunctionResult6.Rotation = SettingsTable.Settings.Rotation
                end
            end)
        elseif FunctionResult5 then
            FunctionResult5.Rotation = 0
        end
    end,
}

AddToggle(Main, SpinOption)

local SizeOption = {
    Name = 'Size',
    MinValue = 10,
    MaxValue = 100,
    Default = 30,
    Increase = 1,
    Callback = function(SizeParameter)
        SettingsTable.Settings.Size = SizeParameter

        local FunctionResult7 = LocalFunction1()

        if FunctionResult7 and not SettingsTable.Settings.Pulse then
            FunctionResult7.Size = UDim2.new(0, SizeParameter, 0, SizeParameter)
        end
    end,
}

AddSlider(Main, SizeOption)

