    Name = 'GunDrop',
    Callback = function()
        if HumanoidRootPart then
            local CharacterInstance = Workspace
            local Descendant1, Descendant2, Descendant3 = ipairs(CharacterInstance:GetDescendants())

            while true do
                local UnknownValue2

                Descendant3, UnknownValue2 = Descendant1(Descendant2, Descendant3)

                if Descendant3 == nil then
                    break
                end
                if UnknownValue2:IsA('Part') and UnknownValue2.Name == 'GunDrop' then
                    firetouchinterest(HumanoidRootPart, UnknownValue2, 0)
                    firetouchinterest(HumanoidRootPart, UnknownValue2, 1)

                    return
                end
            end

            MakeNotifi({
                Title = 'Gun not Found',
                Text = '',
                Time = 2,
            })
        end
    end,
}

