    Function4()
    Function1()
end)
PlayerService.PlayerAdded:Connect(function(Parameter5)
    Parameter5.CharacterAdded:Connect(function()
        wait(0.1)
        Function4()
    end)
end)

local function Function3()
    if MoveDirection then
        MoveDirection:Destroy()

        MoveDirection = nil
    end
end
local function Function4(ParentObject)
    Function3()

    if ErrorMessage ~= 'Text' then
        if ErrorMessage == 'Highlight' then
            local HighlightObject = Instance.new('Highlight', ParentObject)

            HighlightObject.FillColor = Color3.fromRGB(255, 0, 0)
            HighlightObject.OutlineColor = Color3.fromRGB(255, 0, 0)
            HighlightObject.OutlineTransparency = 0
            HighlightObject.FillTransparency = 0.5
            MoveDirection = HighlightObject
        end
    else
        local BillboardGui = Instance.new('BillboardGui', ParentObject)

        BillboardGui.Adornee = ParentObject
        BillboardGui.Size = UDim2.new(0, 100, 0, 50)
        BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
        BillboardGui.AlwaysOnTop = true

        local TextLabel = Instance.new('TextLabel', BillboardGui)

        TextLabel.Size = UDim2.new(1, 0, 1, 0)
        TextLabel.BackgroundTransparency = 1
        TextLabel.Text = 'THE GUN HERE!'
        TextLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
        TextLabel.TextScaled = true
        MoveDirection = BillboardGui
    end
end
local function Function5()
    while GameOption1 do
        local GunDrop = Workspace:FindFirstChild('GunDrop', true)

        if GunDrop then
            if not MoveDirection then
                Function4(GunDrop)
            end
        else
            Function3()
        end

        task.wait(0.5)
    end
end
local function Function6()
    while GameOption2 do
        local TrapObject = Workspace:FindFirstChild('Trap', true)

        if TrapObject and TrapObject:IsDescendantOf(Workspace) then
            if not TargetGame then
                local TrapBillboard = Instance.new('BillboardGui', TrapObject)

                TrapBillboard.Adornee = TrapObject
                TrapBillboard.Size = UDim2.new(0, 100, 0, 50)
                TrapBillboard.StudsOffset = Vector3.new(0, 2, 0)
                TrapBillboard.AlwaysOnTop = true

                local TrapLabel = Instance.new('TextLabel', TrapBillboard)

                TrapLabel.Size = UDim2.new(2, 0, 2, 0)
                TrapLabel.BackgroundTransparency = 1
                TrapLabel.Text = 'Trap here watch out. '
                TrapLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                TrapLabel.TextScaled = true
                TargetGame = TrapBillboard
            end
        elseif TargetGame then
            TargetGame:Destroy()

            TargetGame = nil
        end

        task.wait(1)
    end
end
local function Function7(Parameter6, Parameter7)
    if Parameter6:FindFirstChild('CoinVisual') and not Parameter6:FindFirstChild('CM') then
        local BoxHandle = Instance.new('BoxHandleAdornment')

        BoxHandle.Name = 'CM'
        BoxHandle.Parent = Parameter6
        BoxHandle.Size = Vector3.new(1.5, 1.5, 1.5)
        BoxHandle.Adornee = Parameter6
        BoxHandle.Color3 = Parameter7
        BoxHandle.AlwaysOnTop = true
        BoxHandle.Transparency = 0.45
        BoxHandle.ZIndex = 10
    end
end
local function Function8()
    while GameOption3 do
        local Workspace = Workspace
        local ChildObject, ChildIterator, ChildData = pairs(Workspace:GetChildren())

        while true do
            local CoinContainer

            ChildData, CoinContainer = ChildObject(ChildIterator, ChildData)

            if ChildData == nil then
                break
            end
            if CoinContainer:IsA('Model') and CoinContainer:FindFirstChild('CoinContainer') then
                local CoinIterator, CoinData, CoinInfo = pairs(CoinContainer.CoinContainer:GetChildren())

                while true do
                    local CoinModel

                    CoinInfo, CoinModel = CoinIterator(CoinData, CoinInfo)

                    if CoinInfo == nil then
                        break
                    end
                    if CoinModel:GetAttribute('CoinID') == 'Coin' then
                        Function7(CoinModel, Color3.fromRGB(255, 255, 0))
                    end
                end
            end
        end

        task.wait(0.5)
    end
end
local function Function9()
    local PlayerService = PlayerService
    local PlayerArray, PlayerIndex, PlayerValue = ipairs(PlayerService:GetPlayers())

    while true do
        local PlayerList1

        PlayerValue, PlayerList1 = PlayerArray(PlayerIndex, PlayerValue)

        if PlayerValue == nil then
            break
        end
        if PlayerList1.Backpack and PlayerList1.Backpack:FindFirstChild('Knife') then
            return PlayerList1
        end
    end

    local PlayerService1 = PlayerService
    local PlayerArray1, PlayerIndex1, PlayerValue1 = ipairs(PlayerService1:GetPlayers())

    while true do
        local PlayerList2

        PlayerValue1, PlayerList2 = PlayerArray1(PlayerIndex1, PlayerValue1)

        if PlayerValue1 == nil then
            break
        end
        if PlayerList2.Character and PlayerList2.Character:FindFirstChild('Knife') then
            return PlayerList2
        end
    end

    if EmptyTable and type(EmptyTable) == 'table' then
        local TableKey, TableValue, TableName = pairs(EmptyTable)

        while true do
            local TableData

            TableName, TableData = TableKey(TableValue, TableName)

            if TableName == nil then
                break
            end
            if TableData and TableData.Role == 'Murderer' then
                local TableObject = PlayerService:FindFirstChild(TableName)

                if TableObject then
                    return TableObject
                end
            end
        end
    end

    return nil
end
