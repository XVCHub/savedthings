AddToggle(Main, {
    Name = 'Noclip',
    Default = false,
    Callback = function(Parameter1)
        GameOptions.N54 = Parameter1

        if Parameter1 then
            RunService.Stepped:Connect(function()
                if GameOptions.N54 then
                    local LocalCharacter1 = PlayerService.LocalPlayer.Character

                    if LocalCharacter1 then
                        local DescendantObject, DescendantObject1, DescendantObject2 = pairs(LocalCharacter1:GetDescendants())

                        while true do
                            local UnknownValue1

                            DescendantObject2, UnknownValue1 = DescendantObject(DescendantObject1, DescendantObject2)

                            if DescendantObject2 == nil then
                                break
                            end
                            if UnknownValue1:IsA('BasePart') then
                                UnknownValue1.CanCollide = false
                            end
                        end
                    end
                end
            end)
        else
            local LocalCharacter2 = PlayerService.LocalPlayer.Character

            if LocalCharacter2 then
                local DescendantObject3, DescendantObject4, DescendantObject5 = pairs(LocalCharacter2:GetDescendants())

                while true do
                    local UnknownValue2

                    DescendantObject5, UnknownValue2 = DescendantObject3(DescendantObject4, DescendantObject5)

                    if DescendantObject5 == nil then
                        break
                    end
                    if UnknownValue2:IsA('BasePart') then
                        UnknownValue2.CanCollide = true
                    end
                end
            end
        end
    end,
})
AddToggle(Main, {
    Name = 'Infinite jump',
    Default = false,
    Callback = function(Parameter2)
        GameOptions.InfJ = Parameter2

        UserInputService.JumpRequest:Connect(function()
            if GameOptions.InfJ then
                local LocalCharacter3 = PlayerService.LocalPlayer.Character

                if LocalCharacter3 then
                    LocalCharacter3 = PlayerService.LocalPlayer.Character:FindFirstChildOfClass('Humanoid')
                end
                if LocalCharacter3 then
                    LocalCharacter3:ChangeState('Jumping')
                end
            end
        end)
    end,
})

local SettingConfig = {
    Name = 'Anti Trap',
    Default = false,
    Callback = function(SettingValue)
        if SettingValue then
            local Vu10Value = CharacterModel

            if Vu10Value then
                Vu10Value = CharacterModel:FindFirstChildOfClass('Humanoid')
            end
            if Vu10Value and Vu10Value.WalkSpeed < 1 then
                Vu10Value.WalkSpeed = 16
            end
        end
    end,
}

AddToggle(Main, SettingConfig)
AddToggle(Main, {
    Name = 'Anti Fling',
    Default = false,
    Callback = function(Parameter3)
        if Parameter3 then
            game:GetService('RunService').RenderStepped:Connect(function()
                local PlayerObject, PlayerObject1, PlayerObject2 = ipairs(game:GetService('Players'):GetPlayers())

                while true do
                    local CharacterModel

                    PlayerObject2, CharacterModel = PlayerObject(PlayerObject1, PlayerObject2)

                    if PlayerObject2 == nil then
                        break
                    end
                    if CharacterModel ~= game.Players.LocalPlayer and CharacterModel.Character then
                        local CharacterChild, CharacterChild1, CharacterChild2 = ipairs(CharacterModel.Character:GetChildren())

                        while true do
                            local Vu30Object

                            CharacterChild2, Vu30Object = CharacterChild(CharacterChild1, CharacterChild2)

                            if CharacterChild2 == nil then
                                break
                            end

                            pcall(function()
                                if Vu30Object:IsA('BasePart') then
                                    Vu30Object.CanCollide = false

                                    local Vu30Property = Vu30Object
                                    local Vu30Property1 = Vu30Object
                                    local VectorOrigin = Vector3.zero

                                    Vu30Property1.RotVelocity = Vector3.zero
                                    Vu30Property.Velocity = VectorOrigin
                                    Vu30Object.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
                                end
                            end)
                        end
                    end
                end
            end)
        end
    end,
})

local SectionName1 = AddSection1(Main, {
    'Teste',
})

SetSection(SectionName1, '')
AddButton(Main, {
    Name = 'ServerHop',
    Callback = function()
        local TeleportService = game:GetService('TeleportService')
        local PlaceInfo = game.PlaceId

        local function LocalFunction(FunctionParameter)
            return HttpService:JSONDecode((game:HttpGet('https://games.roblox.com/v1/games/' .. PlaceInfo .. '/servers/Public?limit=100' .. (FunctionParameter and '&cursor=' .. FunctionParameter or ''))))
        end

        local PlaceProperty = PlaceInfo
        local NullValue = nil

        while true do
            local FunctionResult = LocalFunction(NullValue)

            NullValue = FunctionResult.nextPageCursor

            local ResultData = FunctionResult.data

            if ResultData then
                ResultData = FunctionResult.data[1]
            end
            if ResultData then
                TeleportService:TeleportToPlaceInstance(PlaceProperty, ResultData.id, PlayerService.LocalPlayer)

                return
            end
        end
    end,
})
AddButton(Main, {
    Name = 'Rejoin',
    Callback = function()
        local TeleportService1 = game:GetService('TeleportService')
        local LocalPlayer1 = PlayerService.LocalPlayer

        TeleportService1:Teleport(game.PlaceId, LocalPlayer1)
    end,
})

