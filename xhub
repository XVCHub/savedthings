AddToggle(Main, {
    Name = 'Auto Spam Emote',
    Default = false,
    Callback = function(Parameter3)
        GameOptions.ET = Parameter3

        if Parameter3 then
            spawn(function()
                while GameOptions.ET do
                    task.wait(0.2)

                    local CharacterInstance2, CharacterInstance3 = pcall(function()
                        local CharacterModel3 = LocalPlayer.Character

                        if CharacterModel3 and (CharacterModel3:FindFirstChild('Humanoid') and CharacterModel3:FindFirstChild('HumanoidRootPart')) then
                            local RemotesInstance = ReplicatedStorage:FindFirstChild('Remotes') and ReplicatedStorage.Remotes:FindFirstChild('Misc')

                            if RemotesInstance then
                                RemotesInstance = ReplicatedStorage.Remotes.Misc:FindFirstChild('PlayEmote')
                            end
                            if RemotesInstance then
                                RemotesInstance:Fire(AnimationList[math.random(1, #AnimationList)])
                            end
                        end
                    end)

                    if not CharacterInstance2 then
                        warn('Auto Emote Error: ' .. tostring(CharacterInstance3))
                    end
                end
            end)
        end
    end,
})
AddToggle(Main, {
    Name = 'Remove Barriers',
    Default = false,
    Callback = function(Parameter4)
        GameOptions.RBB = Parameter4

        if GameOptions.RBB then
            task.spawn(function()
                while GameOptions.RBB do
                    local WorkspaceInstance = Workspace
                    local Descendant4, Descendant5, Descendant6 = pairs(WorkspaceInstance:GetDescendants())

                    while true do
                        local UnknownValue7

                        Descendant6, UnknownValue7 = Descendant4(Descendant5, Descendant6)

                        if Descendant6 == nil then
                            break
                        end
                        if UnknownValue7.Name == 'GlitchProof' or UnknownValue7.Name == 'KillBrick' then
                            UnknownValue7:Destroy()
                        end
                    end

                    task.wait(180)
                end
            end)
        end
    end,
})

