AddToggle(Main, {
    Name = 'Auto Hide Coin',
    Default = false,
    Callback = function(Parameter6)
        GameOptions.cR = Parameter6

        if Parameter6 then
            task.spawn(function()
                while GameOptions.cR do
                    local WorkspaceService = Workspace
                    local DescendantList, DescendantIndex, DescendantKey = pairs(WorkspaceService:GetDescendants())

                    while true do
                        local ChildrenList

                        DescendantKey, ChildrenList = DescendantList(DescendantIndex, DescendantKey)

                        if DescendantKey == nil then
                            break
                        end
                        if ChildrenList.Name == 'CoinContainer' then
                            local ChildIndex, ChildKey, ChildObject = pairs(ChildrenList:GetChildren())

                            while true do
                                local UnusedVariable2

                                ChildObject, UnusedVariable2 = ChildIndex(ChildKey, ChildObject)

                                if ChildObject == nil then
                                    break
                                end
                                if UnusedVariable2.Name == 'Coin_Server' then
                                    UnusedVariable2:Destroy()
                                end
                            end

                            if #ChildrenList:GetChildren() == 0 then
                                ChildrenList:Destroy()
                            end
                        end
                    end

                    task.wait(5)
                end
            end)
        end
    end,
})
