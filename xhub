local TeleportToClosestPlayerConfig = {
    Name = 'Teleport To closest Player',
    Callback = function()
        local MaxValue = math.huge
        local PlayersService3 = PlayerService
        local Player7, Player8, Player9 = pairs(PlayersService3:GetPlayers())
        local NilValue = nil

        while true do
            local UnusedVariable9

            Player9, UnusedVariable9 = Player7(Player8, Player9)

            if Player9 == nil then
                break
            end
            if UnusedVariable9 ~= LocalPlayer and UnusedVariable9.Character and UnusedVariable9.Character:FindFirstChild('HumanoidRootPart') then
                local Distance = (HumanoidRootPart.Position - UnusedVariable9.Character.HumanoidRootPart.Position).Magnitude

                if Distance < MaxValue then
                    NilValue = UnusedVariable9
                    MaxValue = Distance
                end
            end
        end

        if NilValue then
            HumanoidRootPart.CFrame = NilValue.Character.HumanoidRootPart.CFrame
        end
    end,
}

AddButton(Main, TeleportToClosestPlayerConfig)

local TeleportOption = {
    Name = 'Teleport To Random Player',
    Callback = function()
        local PlayerService = PlayerService
        local PlayerList, PlayerIndex, PlayerKey = pairs(PlayerService:GetPlayers())
        local EmptyTable = {}

        while true do
            local UnusedVariable

            PlayerKey, UnusedVariable = PlayerList(PlayerIndex, PlayerKey)

            if PlayerKey == nil then
                break
            end
            if UnusedVariable ~= LocalPlayer and UnusedVariable.Character and UnusedVariable.Character:FindFirstChild('HumanoidRootPart') then
                table.insert(EmptyTable, UnusedVariable)
            end
        end

        if #EmptyTable > 0 then
            HumanoidRootPart.CFrame = EmptyTable[math.random(1, #EmptyTable)].Character.HumanoidRootPart.CFrame
        end
    end,
}

AddButton(Main, TeleportOption)

