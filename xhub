    Name = 'Grab Gun Button ',
    Default = false,
    Callback = function(Parameter)
        if Parameter then
            NewButton('Grab Gun')
            Pos(0.4, 0, 0, 0)
            Function(function()
                local LocalCharacter = game.Players.LocalPlayer.Character
                local HumanoidRootPart = LocalCharacter and LocalCharacter:FindFirstChild('HumanoidRootPart')

                if HumanoidRootPart then
                    local Descendant1, Descendant2, Descendant3 = pairs(game.Workspace:GetDescendants())

                    while true do
                        local UnknownVariable

                        Descendant3, UnknownVariable = Descendant1(Descendant2, Descendant3)

                        if Descendant3 == nil then
                            break
                        end
                        if UnknownVariable:IsA('Part') and UnknownVariable.Name == 'GunDrop' then
                            firetouchinterest(HumanoidRootPart, UnknownVariable, 0)
                            firetouchinterest(HumanoidRootPart, UnknownVariable, 1)

                            return
                        end
                    end

                    MakeNotifi({
                        Title = 'Gun not Found',
                        Text = '',
                        Time = 2,
                    })
                end
            end)
        elseif UnknownValue12 then
            UnknownValue12:Destroy()
        end
    end,
}

